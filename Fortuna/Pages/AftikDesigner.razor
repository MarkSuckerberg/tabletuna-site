@page "/AftikDesigner"
@using BlazorDownloadFile
<h2>Aftik Designer</h2>

<div class="character-display">
<div class="column-container">
<div class="column-left">
    <label>
        Main Colour
        <input type="color" @bind="_mainColour"/>
    </label>
    <br/>
    <label>
        Accents
        <input type="color" @bind="_accentColour"/>
    </label>
    <br/>
    <label>
        Scarf 1
        <input type="color" @bind="_scarfColour1"/>
    </label>
    <br/>
    <label>
        Scarf 2
        <input type="color" @bind="_scarfColour2"/>
    </label>
    <br/>
    <label>
        Shirt
        <input type="color" @bind="_shirtColour"/>
    </label>
    <br/>
    <button @onclick="YouWouldNotDownloadAnAftik">Download SVG</button>
    <br/>
    <a href="https://cosmosdex.com/cosmosdex/species/aftik" target="_blank" rel="noreferrer noopener">Learn more about Aftiks!</a>
</div>
<div class="column-right">
